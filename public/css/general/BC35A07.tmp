/*@start: main navigation of content area*/
.app .navigation{
	position: relative;
	height: 40px;
	width: 916px;
	top: 5px;
	left: 3px;
	padding: 0 15px;
	overflow: hidden;
}
.app .navigation li{
	float: left;
}

.app .navigation li.enterpi{
	background: url("images/nav.png") no-repeat 100% -50px; 
	height: 43px;
	z-index: 8;
	position: absolute;
	top: 1px;
}
.app .navigation li.enterpi .left{
	background: url("images/nav.png") no-repeat 0 -1px;
	height: 38px;
	margin-right: 7px;
}
.app .navigation li.enterpi .mid{
	background: url("images/nav.png") no-repeat center -150px;
	height: 30px;
}
.app .navigation li.enterpi.act .mid{
	height: 43px;
	margin-left: 7px;
}

.app .navigation li a{
	position: relative;
	z-index: 10; 
	letter-spacing: 0;
	float: left; 
	display: block;
	/*margin: 0 16px;*/
	color: #c4c8cd;
	text-shadow: 0 1px 0 #282d30;
	font-size: 14px;
	padding: 7px 16px 7px 38px;
	-webkit-transition: color, background 650ms;
	-moz-transition: color, background 650ms;
	cursor: pointer;
}
.app .navigation li.cur a{
	color: #fff;
}
.app .navigation li:hover a{
	color: #fff;
}

.app .navigation li.l{
	margin-left: 0px;
}


